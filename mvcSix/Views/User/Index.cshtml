@model List<User>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<table class="table">
    <tr>
        <th>userId</th>
        <th>name</th>
        <th>email</th>
        <th>role</th>
        <th>gender</th>
        <th>contact</th>
        <th>address</th>
    </tr>
    <tbody>
@foreach(var m in Model)
{
    <tr>
                <td>@m.UserId</td>
                <td>@m.Name</td>
                <td>@m.Email</td>
                <td>@m.Role</td>
                <td>@m.Gender</td>
                <td>@m.Contact</td>
                <td>@m.Address</td>
                <td> <a href="/User/Edit/@m.UserId">Edit</a></td>
    </tr>
}
    </tbody>

</table>